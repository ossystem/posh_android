<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="ru.jufy.myposh.activities.LoginActivity">

    <FrameLayout
        android:id = "@+id/fragment_frame"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/login_fragment_margin_vertical"
        android:layout_marginTop="@dimen/login_fragment_margin_vertical"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginStart="@dimen/login_fragment_margin_horizontal"
        android:layout_marginEnd="@dimen/login_fragment_margin_horizontal">
    </FrameLayout>

</android.support.constraint.ConstraintLayout>
